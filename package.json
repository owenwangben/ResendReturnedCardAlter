{
	"name": "mma-card",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng-serve": "ng serve --open --proxy-config proxy.conf.json --host 0.0.0.0",
		"ng-serve:mobile": "ng serve --open --proxy-config proxy.conf.json --app mobile --host 0.0.0.0",
		"ng-serve:local": "ng serve --open --proxy-config proxy.conf.local.json --host 0.0.0.0",
		"ng-serve:local:mobile": "ng serve --open --proxy-config proxy.conf.local.json --app mobile --host 0.0.0.0",
		"ng-build": "ng build --bh /SinoCard/ --deploy /SinoCard/ --prod --output-hashing none",
		"ng-build:mobile": "ng build --bh /m/SinoCard/ --deploy /m/SinoCard/ --prod --app mobile --output-hashing none",
		"start": "run-p watch-scss:web ng-serve",
		"start:mobile": "run-p watch-scss:mobile ng-serve:mobile",
		"start:local": "run-p watch-scss:web ng-serve:local",
		"start:local:mobile": "run-p watch-scss:mobile ng-serve:local:mobile",
		"compile-scss": "run-p compile-scss:web compile-scss:mobile",
		"compile-scss:web": "node-sass -r src/scss/web -o src/mma8/card/css-eweb",
		"compile-scss:mobile": "node-sass -r src/scss/mobile -o src/mma8/card/css-mweb",
		"watch-scss": "run-p watch-scss:web watch-scss:mobile",
		"watch-scss:web": "onchange \"**/scss/web/*.scss\" -i -- run-s compile-scss:web",
		"watch-scss:mobile": "onchange \"**/scss/mobile/*.scss\" -i -- run-s compile-scss:mobile",
		"build": "run-p build:web build:mobile",
		"build:web": "run-s compile-scss:web ng-build",
		"build:mobile": "run-s compile-scss:mobile ng-build:mobile",
		"install-build-tools": "npm install --global --production windows-build-tools",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"postinstall": "",
		"rmdir-dist": "powershell if (test-path dist) { remove-item dist -force -recurse }",
		"rmdir-dist-mobile": "powershell if (test-path dist-mobile) { remove-item dist-mobile -force -recurse }",
		"mock-service": "cd osprey-mock-service && start cmd /c osprey-mock-service -f api.raml -p 3000",
		"prestart": "run-p rmdir-dist mock-service",
		"prestart:mobile": "run-p rmdir-dist-mobile mock-service",
		"prestart:local": "run-p rmdir-dist mock-service",
		"prestart:local:mobile": "run-p rmdir-dist-mobile mock-service"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^5.2.0",
		"@angular/common": "^5.2.0",
		"@angular/compiler": "^5.2.0",
		"@angular/core": "^5.2.0",
		"@angular/forms": "^5.2.0",
		"@angular/http": "^5.2.0",
		"@angular/platform-browser": "^5.2.0",
		"@angular/platform-browser-dynamic": "^5.2.0",
		"@angular/router": "^5.2.0",
		"@ng-idle/core": "^2.0.0-beta.15",
		"@ng-idle/keepalive": "^2.0.0-beta.15",
		"core-js": "^2.4.1",
		"devextreme": "^18.2.8",
		"devextreme-angular": "^18.2.8",
		"file-saver": "^1.3.3",
		"js-sha256": "^0.9.0",
		"moment": "^2.29.4",
		"ng2-file-upload": "^1.3.0",
		"ngx-mask": "^8.2.0",
		"ngx-pagination": "^5.1.1",
		"node-sass": "^4.12.0",
		"rxjs": "^5.5.6",
		"sweetalert2": "^6.11.5",
		"zone.js": "^0.8.19"
	},
	"devDependencies": {
		"@angular/cli": "~1.7.3",
		"@angular/compiler-cli": "^5.2.0",
		"@angular/language-service": "^5.2.0",
		"@types/file-saver": "^1.3.0",
		"@types/jasmine": "~2.8.3",
		"@types/jasminewd2": "~2.0.2",
		"@types/node": "~6.0.60",
		"codelyzer": "^4.0.1",
		"jasmine-core": "~2.8.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~2.0.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"npm-run-all": "^4.1.2",
		"onchange": "^3.3.0",
		"osprey-mock-service": "^0.2.1",
		"protractor": "~5.1.2",
		"ts-node": "~4.1.0",
		"tslint": "^5.9.1",
		"typescript": "~2.5.3"
	}
}
